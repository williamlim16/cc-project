generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Menu {
  id     String  @id @default(cuid())
  name   String
  orders Order[]
}

model Order {
  id    String  @id @default(cuid())
  name  String
  menus Menu[]
  done  Boolean @default(false)
}
