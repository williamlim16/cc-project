generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Menu {
  id     String  @id @default(cuid())
  name   String
  orders Order[]
}

model Order {
  id        String   @id @default(cuid())
  name      String
  menus     Menu[]
  done      Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
